{
  "name": "kamigotchi",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "packageManager": "pnpm@8.14.1",
  "scripts": {
    "dev": "run-pty % pnpm contracts:node % pnpm client:dev % pnpm contracts:dev",
    "nuke": "pnpm nuke:client && pnpm nuke:contracts && rimraf node_modules pnpm-lock* yarn.lock",
    "client:build": "pnpm --filter=client build",
    "client:dev": "pnpm --filter=client dev",
    "client:nuke": "pnpm --filter=client nuke",
    "contracts:build": "pnpm --filter=contracts build",
    "contracts:dev": "pnpm --filter=contracts dev",
    "contracts:node": "pnpm --filter=contracts devnode",
    "contracts:nuke": "pnpm --filter=contracts nuke"
  },
  "devDependencies": {
    "buffer": "^5.5.0",
    "events": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "path-browserify": "^1.0.0",
    "punycode": "^1.4.1",
    "querystring-es3": "^0.2.1",
    "rimraf": "^5.0.5",
    "run-pty": "^4.0.3",
    "threads": "^1.7.0",
    "url": "^0.11.0"
  },
  "pnpm": {
    "patchedDependencies": {
      "threads@1.7.0": "patches/threads@1.7.0.patch"
    }
  }
}